            }
          }
          generate_page_nav(first_form_view30, '30', form_view_count30, form_view_max30);
        }
      });
      function check_required30(but_type) {
        if (but_type == 'reset') {
          if (window.before_reset) {
            before_reset();
          }
         var types = ["type_name","type_submitter_mail","type_text","type_submit_reset","type_textarea"];
			var label_ids = ["1","2","3","4","5"];
			jQuery.each(types, function (index, elem) {
				switch(elem) {
					case "type_text":
					case "type_password":
					case "type_textarea":
					case "type_number":
					case "type_submitter_mail":	
					case "type_spinner":
					case 'type_own_select':
					case 'type_country':
					case 'type_date': 
					case 'type_hidden': 
						jQuery("#wdform_"+label_ids[index]+"_element30").val('');
					break;
						
					case 'type_send_copy':	
						jQuery("#wdform_"+label_ids[index]+"_element30").prop('checked', false);
					break;
					case 'type_phone':	
						jQuery("#wdform_"+label_ids[index]+"_element_first30, #wdform_"+label_ids[index]+"_element_last30").val('');
					break;
					
					case 'type_name':	
						jQuery("#wdform_"+label_ids[index]+"_element_first30, #wdform_"+label_ids[index]+"_element_last30, #wdform_"+label_ids[index]+"_element_title30, #wdform_"+label_ids[index]+"_element_middle30").val('');
					break;
					
					case 'type_address':
						jQuery("#wdform_"+label_ids[index]+"_street130, #wdform_"+label_ids[index]+"_street230, #wdform_"+label_ids[index]+"_city30, #wdform_"+label_ids[index]+"_state30, #wdform_"+label_ids[index]+"_postal30, #wdform_"+label_ids[index]+"_country30").val('');
					break;
					
					case 'type_checkbox':
						jQuery("#form30 div[wdid='"+label_ids[index]+"'] .checkbox-div input").prop('checked', false);
						jQuery("#wdform_"+label_ids[index]+"_other_br30").remove();
						jQuery("#wdform_"+label_ids[index]+"_other_input30").remove();
					break;
					
					case 'type_radio':
						jQuery("#form30 div[wdid='"+label_ids[index]+"'] .radio-div input").prop('checked', false);
						jQuery("#wdform_"+label_ids[index]+"_other_br30").remove();
						jQuery("#wdform_"+label_ids[index]+"_other_input30").remove();
					break;
           
					case 'type_time':
						jQuery("#wdform_"+label_ids[index]+"_hh30, #wdform_"+label_ids[index]+"_mm30, #wdform_"+label_ids[index]+"_ss30, #wdform_"+label_ids[index]+"_am_pm30").val('');
					break;
            
					case 'type_date_fields': 
						jQuery("#wdform_"+label_ids[index]+"_day30, #wdform_"+label_ids[index]+"_month30, #wdform_"+label_ids[index]+"_year30").val('');
					break;

					case 'type_file_upload':
						jQuery("#wdform_"+label_ids[index]+"_element30_save").remove();
					break;
            
					case 'type_paypal_price': 
						jQuery("#wdform_"+label_ids[index]+"_element_dollars30, #wdform_"+label_ids[index]+"_element_cents30").val('');
					break;
            
					case 'type_paypal_select': 
						jQuery("#wdform_"+label_ids[index]+"_element30, #wdform_"+label_ids[index]+"_element_quantity30, #form30 div[wdid='"+label_ids[index]+"'] .paypal-property select").val('');
					break;
					
					case 'type_paypal_radio': 
						jQuery("#wdform_"+label_ids[index]+"_element_quantity30,#form30 div[wdid='"+label_ids[index]+"'] .paypal-property select").val('');
						jQuery("#form30 div[wdid='"+label_ids[index]+"'] .radio-div input").prop('checked', false);
					break;
					
					case 'type_paypal_shipping':
						jQuery("#form30 div[wdid='"+label_ids[index]+"'] .radio-div input").prop('checked', false);
					break;
            
					case 'type_paypal_checkbox':
						jQuery("#wdform_"+label_ids[index]+"_element_quantity30,#form30 div[wdid='"+label_ids[index]+"'] .paypal-property select").val('');
						jQuery("#form30 div[wdid='"+label_ids[index]+"'] .checkbox-div input").prop('checked', false);
					break;
				
					case 'type_star_rating': 
						jQuery("#wdform_"+label_ids[index]+"_selected_star_amount30").val('');
						jQuery("#wdform_"+label_ids[index]+"_element30 img").attr('src', plugin_url+'/images/star.png');
					break;
					
					case 'type_scale_rating': 
						jQuery("#form30 div[wdid='"+label_ids[index]+"'] .radio-div input").prop('checked', false);
					break;	

					case 'type_slider':
						jQuery("#wdform_"+label_ids[index]+"_element30").slider({
							value: eval(0),
						});
						jQuery("#wdform_"+label_ids[index]+"_element_value30").html('0');
					break;	
					
					case 'type_range':
						jQuery("#wdform_"+label_ids[index]+"_element300, #wdform_"+label_ids[index]+"_element301").val('');					
					break;	
					
					case 'type_grading': 
						jQuery("#wdform_"+label_ids[index]+"_element30 input").val('');
					break;
					
					case 'type_matrix':
						jQuery("#wdform_"+label_ids[index]+"_element30 .radio-div input").prop('checked', false);
						jQuery("#wdform_"+label_ids[index]+"_element30 .checkbox-div input").prop('checked', false);
						jQuery("#wdform_"+label_ids[index]+"_element30 input[type='text']").val('');
						jQuery("#wdform_"+label_ids[index]+"_element30 select").val('');
					break;
					
					case 'type_paypal_total': 
						jQuery("#wdform_"+label_ids[index]+"div_total30").html('$0');	
						jQuery("#wdform_"+label_ids[index]+"paypal_products30").empty();	
					break;	
					default:
					break;
				}
			});
			
          return;
        }
        if (window.before_submit) {
          before_submit();
        }
        x = jQuery("#form30");
        
                  if(x.find(jQuery("div[wdid=1]")).length != 0 && x.find(jQuery("div[wdid=1]")).css("display") != "none")
                  {
                    if(jQuery("#wdform_1_element_first30").val()=="" || jQuery("#wdform_1_element_first30").val()=="" || jQuery("#wdform_1_element_last30").val()=="" || jQuery("#wdform_1_element_last30").val()=="" || (jQuery("#wdform_1_element_title30").length != 0 && (jQuery("#wdform_1_element_title30").val()=="" || jQuery("#wdform_1_element_title30").val()=="")) || (jQuery("#wdform_1_element_middle30").length != 0 && (jQuery("#wdform_1_element_middle30").val()=="" || jQuery("#wdform_1_element_middle30").val()=="")))
                    {
						alert("Name: field is required.");
						old_bg=x.find(jQuery("div[wdid=1]")).css("background-color");
						x.find(jQuery("div[wdid=1]")).effect( "shake", {}, 500 ).css("background-color","#FF8F8B").animate({backgroundColor: old_bg}, {duration: 500, queue: false });
						jQuery("#wdform_1_element_first30").focus();
						return false;
                    }
                  }
                  
              if(x.find(jQuery("div[wdid=2]")).length != 0 && x.find(jQuery("div[wdid=2]")).css("display") != "none")
              {
              
              if(jQuery("#wdform_2_element30").val()!="" && jQuery.trim(jQuery("#wdform_2_element30").val()).search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) == -1 )
                {
                  alert("This is not a valid email address.");
                  old_bg=x.find(jQuery("div[wdid=2]")).css("background-color");
                  x.find(jQuery("div[wdid=2]")).effect( "shake", {}, 500 ).css("background-color","#FF8F8B").animate({backgroundColor: old_bg}, {duration: 500, queue: false });
                  jQuery("#wdform_2_element30").focus();
                  return false;
                }
              
              }
              ;
        var a = [];
        if (typeof a[30] !== 'undefined' && a[30] == 1) {
          return;
        }
        
        var disabled_fields ="";	
        jQuery("div[wdid]").each(function() {
          if(jQuery(this).css("display")=="none")
          {		
            disabled_fields += jQuery(this).attr("wdid");
            disabled_fields += ",";
          }	

            if(disabled_fields)
            jQuery("<input type=\"hidden\" name=\"disabled_fields30\" value =\""+disabled_fields+"\" />").appendTo("#form30");
            
        });
        a[30] = 1;
        document.getElementById("form"+30).submit();
      }
      function check30(id) {
        x = jQuery("#30form_view"+id);
        
                  if(x.find(jQuery("div[wdid=1]")).length != 0 && x.find(jQuery("div[wdid=1]")).css("display") != "none")
                  {
                    if(jQuery("#wdform_1_element_first30").val()=="" || jQuery("#wdform_1_element_first30").val()=="" || jQuery("#wdform_1_element_last30").val()=="" || jQuery("#wdform_1_element_last30").val()=="" || (jQuery("#wdform_1_element_title30").length != 0 && (jQuery("#wdform_1_element_title30").val()=="" || jQuery("#wdform_1_element_title30").val()=="")) || (jQuery("#wdform_1_element_middle30").length != 0 && (jQuery("#wdform_1_element_middle30").val()=="" || jQuery("#wdform_1_element_middle30").val()=="")))
                    {
						alert("Name: field is required.");
						old_bg=x.find(jQuery("div[wdid=1]")).css("background-color");
						x.find(jQuery("div[wdid=1]")).effect( "shake", {}, 500 ).css("background-color","#FF8F8B").animate({backgroundColor: old_bg}, {duration: 500, queue: false });
						jQuery("#wdform_1_element_first30").focus();
						return false;
                    }
                  }
                  
              if(x.find(jQuery("div[wdid=2]")).length != 0 && x.find(jQuery("div[wdid=2]")).css("display") != "none")
              {
              
              if(jQuery("#wdform_2_element30").val()!="" && jQuery.trim(jQuery("#wdform_2_element30").val()).search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) == -1 )
                {
                  alert("This is not a valid email address.");
                  old_bg=x.find(jQuery("div[wdid=2]")).css("background-color");
                  x.find(jQuery("div[wdid=2]")).effect( "shake", {}, 500 ).css("background-color","#FF8F8B").animate({backgroundColor: old_bg}, {duration: 500, queue: false });
                  jQuery("#wdform_2_element30").focus();
                  return false;
                }
              
              }
              ;
        return true;
      }
